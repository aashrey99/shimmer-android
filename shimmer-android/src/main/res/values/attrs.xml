<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ShimmerFrameLayout">
    <attr name="fbs_auto_start" format="boolean"/>
    <attr name="fbs_base_alpha" format="float"/>
    <attr name="fbs_duration" format="integer"/>
    <attr name="fbs_repeat_count" format="integer"/>
    <attr name="fbs_repeat_delay" format="integer"/>
    <attr name="fbs_repeat_mode" format="enum">
      <enum name="fbs_restart" value="1"/>
      <enum name="fbs_reverse" value="2"/>
    </attr>
    <attr name="fbs_angle" format="enum">
      <enum name="fbs_cw_0" value="0"/>
      <enum name="fbs_cw_90" value="90"/>
      <enum name="fbs_cw_180" value="180"/>
      <enum name="fbs_cw_270" value="270"/>
    </attr>
    <attr name="fbs_dropoff" format="float"/>
    <attr name="fbs_fixed_width" format="dimension"/>
    <attr name="fbs_fixed_height" format="dimension"/>
    <attr name="fbs_intensity" format="float"/>
    <attr name="fbs_relative_width" format="float"/>
    <attr name="fbs_relative_height" format="float"/>
    <attr name="fbs_shape" format="enum">
      <enum name="fbs_linear" value="0"/>
      <enum name="fbs_radial" value="1"/>
    </attr>
    <attr name="fbs_tilt" format="float"/>
  </declare-styleable>
</resources>
